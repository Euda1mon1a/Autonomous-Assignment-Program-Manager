{
  "name": "@scheduler/agent-adk",
  "version": "0.1.0",
  "description": "Google ADK agents for Residency Scheduler with Gemini and evaluation framework",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "npx @google/adk-devtools web src/agents/index.ts",
    "cli": "npx @google/adk-devtools run src/agents/index.ts",
    "test": "vitest",
    "test:eval": "vitest run --config vitest.eval.config.ts",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@google/adk": "^0.2.0",
    "@google-cloud/vertexai": "^1.9.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@google/adk-devtools": "^0.2.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.9.0",
    "vitest": "^2.0.0",
    "eslint": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0"
  },
  "engines": {
    "node": ">=20.12.0"
  }
}
