{
  "dashboard": {
    "id": null,
    "uid": "celery-tasks",
    "title": "Celery Background Tasks",
    "description": "Monitoring for Celery workers, queues, and task execution",
    "tags": ["celery", "tasks", "workers"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
    },
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus",
          "current": {
            "value": "Prometheus",
            "text": "Prometheus"
          }
        },
        {
          "name": "queue",
          "type": "query",
          "datasource": "${datasource}",
          "query": "label_values(celery_queue_length, queue)",
          "multi": true,
          "includeAll": true,
          "current": {
            "text": "All",
            "value": "$__all"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "type": "stat",
        "title": "Active Workers",
        "targets": [
          {
            "expr": "celery_workers",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 2,
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "type": "stat",
        "title": "Total Queue Depth",
        "targets": [
          {
            "expr": "sum(celery_queue_length)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 50, "color": "yellow"},
                {"value": 100, "color": "orange"},
                {"value": 500, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 3,
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "type": "stat",
        "title": "Task Success Rate (5m)",
        "targets": [
          {
            "expr": "rate(celery_task_success_total[5m]) / (rate(celery_task_success_total[5m]) + rate(celery_task_failure_total[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.9, "color": "yellow"},
                {"value": 0.95, "color": "green"}
              ]
            }
          }
        },
        "options": {
          "colorMode": "background"
        }
      },
      {
        "id": 4,
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "type": "stat",
        "title": "Tasks Running",
        "targets": [
          {
            "expr": "sum(celery_tasks_active)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"}
              ]
            }
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 5,
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "type": "timeseries",
        "title": "Task Success/Failure Rates",
        "targets": [
          {
            "expr": "rate(celery_task_success_total[5m])",
            "refId": "A",
            "legendFormat": "Success - {{task}}"
          },
          {
            "expr": "rate(celery_task_failure_total[5m])",
            "refId": "B",
            "legendFormat": "Failure - {{task}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10,
              "lineWidth": 2
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": "Failure.*"},
              "properties": [
                {
                  "id": "color",
                  "value": {"mode": "fixed", "fixedColor": "red"}
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["last", "mean"]
          }
        }
      },
      {
        "id": 6,
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "type": "timeseries",
        "title": "Queue Depths by Queue",
        "targets": [
          {
            "expr": "celery_queue_length{queue=~\"$queue\"}",
            "refId": "A",
            "legendFormat": "{{queue}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20,
              "lineWidth": 2
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["last", "max"]
          }
        }
      },
      {
        "id": 7,
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 18
        },
        "type": "timeseries",
        "title": "Task Duration (p50, p95, p99)",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(celery_task_runtime_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "p99"
          },
          {
            "expr": "histogram_quantile(0.95, rate(celery_task_runtime_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(celery_task_runtime_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "p50"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 8,
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 12,
          "y": 18
        },
        "type": "timeseries",
        "title": "Active Tasks by Worker",
        "targets": [
          {
            "expr": "celery_tasks_active",
            "refId": "A",
            "legendFormat": "{{worker}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20,
              "lineWidth": 2
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["last", "mean", "max"]
          }
        }
      },
      {
        "id": 9,
        "gridPos": {
          "h": 10,
          "w": 24,
          "x": 0,
          "y": 28
        },
        "type": "table",
        "title": "Task Statistics by Task Name",
        "targets": [
          {
            "expr": "sum by (task) (rate(celery_task_success_total[1h]))",
            "refId": "A",
            "format": "table",
            "instant": true
          },
          {
            "expr": "sum by (task) (rate(celery_task_failure_total[1h]))",
            "refId": "B",
            "format": "table",
            "instant": true
          },
          {
            "expr": "histogram_quantile(0.95, sum by (task, le) (rate(celery_task_runtime_seconds_bucket[1h])))",
            "refId": "C",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "Value #A": "Success Rate",
                "Value #B": "Failure Rate",
                "Value #C": "p95 Duration",
                "task": "Task Name"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "left"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Success Rate"},
              "properties": [
                {"id": "unit", "value": "ops"},
                {"id": "decimals", "value": 3}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Failure Rate"},
              "properties": [
                {"id": "unit", "value": "ops"},
                {"id": "decimals", "value": 3},
                {
                  "id": "custom.cellOptions",
                  "value": {
                    "type": "color-background",
                    "mode": "gradient"
                  }
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      {"value": 0, "color": "green"},
                      {"value": 0.01, "color": "yellow"},
                      {"value": 0.1, "color": "red"}
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "p95 Duration"},
              "properties": [
                {"id": "unit", "value": "s"},
                {"id": "decimals", "value": 2}
              ]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "Success Rate",
              "desc": true
            }
          ]
        }
      },
      {
        "id": 10,
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 38
        },
        "type": "timeseries",
        "title": "Resilience Health Check Task Performance",
        "targets": [
          {
            "expr": "rate(celery_task_success_total{task=~\".*resilience.*health.*\"}[5m])",
            "refId": "A",
            "legendFormat": "Success - {{task}}"
          },
          {
            "expr": "rate(celery_task_failure_total{task=~\".*resilience.*health.*\"}[5m])",
            "refId": "B",
            "legendFormat": "Failure - {{task}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 11,
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 12,
          "y": 38
        },
        "type": "timeseries",
        "title": "N-1/N-2 Analysis Task Performance",
        "targets": [
          {
            "expr": "rate(celery_task_success_total{task=~\".*contingency.*\"}[5m])",
            "refId": "A",
            "legendFormat": "Success - {{task}}"
          },
          {
            "expr": "rate(celery_task_failure_total{task=~\".*contingency.*\"}[5m])",
            "refId": "B",
            "legendFormat": "Failure - {{task}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 12,
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 48
        },
        "type": "stat",
        "title": "Total Tasks Processed (24h)",
        "targets": [
          {
            "expr": "sum(increase(celery_task_success_total[24h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0
          }
        },
        "options": {
          "colorMode": "value"
        }
      },
      {
        "id": 13,
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 48
        },
        "type": "stat",
        "title": "Total Task Failures (24h)",
        "targets": [
          {
            "expr": "sum(increase(celery_task_failure_total[24h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "colorMode": "background"
        }
      },
      {
        "id": 14,
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 48
        },
        "type": "stat",
        "title": "Avg Task Duration (1h)",
        "targets": [
          {
            "expr": "rate(celery_task_runtime_seconds_sum[1h]) / rate(celery_task_runtime_seconds_count[1h])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 60, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "colorMode": "value"
        }
      }
    ]
  }
}
